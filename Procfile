web: sh -c "PY=$([ -x /app/.venv/bin/python ] && echo /app/.venv/bin/python || (command -v python3 >/dev/null 2>&1 && echo python3 || echo python)); DJANGO_SETTINGS_MODULE=NESAKO.settings "$PY" -m django collectstatic --noinput && exec "$PY" -m gunicorn wsgi:application --bind 0.0.0.0:${PORT:-8080}"
